    "@types/codemirror": "^5.60.15",
    "@types/css-modules": "^1.0.5",
    "@types/jest": "^29.5.14",
    "@types/markdown-it": "^14.1.2",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "9.x",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
