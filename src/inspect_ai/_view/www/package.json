    "@mui/material": "^7.1.0",
    "@mui/x-tree-view": "^8.3.0",
    "@popperjs/core": "^2.11.8",
    "ansi-output": "^0.0.9",
    "asciinema-player": "^3.9.0",
    "bootstrap": "^5.3.6",
    "bootstrap-icons": "^1.12.1",
    "clipboard": "^2.0.11",
